    "express": "^4.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@mini-git-gui/web-gui": "0.4.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}